{
    "name": "inventory-source-connector",
    "config": {
        "connector.class" : "io.debezium.connector.oracle.OracleConnector",
        "tasks.max" : "1",
        "database.server.name" : "oracle-db-source",
        "database.hostname" : "oracle-db-source",
        "database.port" : "1521",
